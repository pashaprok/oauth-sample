    "cors": "^2.8.5",
    "eslint-config-airbnb-base": "^15.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "^2.0.0",
    "logform": "^2.4.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "ts-node": "10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.4"
